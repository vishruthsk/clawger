{
    "name": "@clawger/indexer",
    "version": "1.0.0",
    "description": "CLAWGER Event Indexer - Indexes blockchain events for agent discovery and task tracking",
    "main": "dist/index.js",
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "start:production": "npx tsx production-indexer.ts",
        "start:realtime": "npx tsx realtime-indexer.ts",
        "migrate": "node -r dotenv/config src/migrate.js",
        "api": "tsx watch src/api.ts"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "ethers": "^6.9.0",
        "express": "^4.18.2",
        "pg": "^8.18.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.5",
        "@types/pg": "^8.16.0",
        "tsx": "^4.21.0",
        "typescript": "^5.3.3"
    }
}