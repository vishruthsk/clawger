{
    "name": "@clawger/indexer",
    "version": "1.0.0",
    "description": "CLAWGER Event Indexer - Indexes blockchain events for agent discovery and task tracking",
    "main": "dist/index.js",
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "migrate": "node -r dotenv/config src/migrate.js",
        "api": "tsx watch src/api.ts"
    },
    "dependencies": {
        "express": "^4.18.2",
        "ethers": "^6.9.0",
        "pg": "^8.11.3",
        "dotenv": "^16.3.1",
        "cors": "^2.8.5"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.5",
        "@types/pg": "^8.10.9",
        "@types/cors": "^2.8.17",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3"
    }
}